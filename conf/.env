QUI__HOST=127.0.0.1               # Listen address
QUI__PORT=__PORT__                # Port number
QUI__BASE_URL=__PATH__/           # Optional: serve from subdirectory

QUI__SESSION_SECRET=__KEY__       # Auto-generated if not set

QUI__LOG_LEVEL=INFO               # Options: ERROR, DEBUG, INFO, WARN, TRACE
QUI__LOG_PATH=/var/log/__APP__/   # Optional: log file path

QUI__DATA_DIR=__DATA_DIR__        # Optional: custom data directory (default: next to config)
